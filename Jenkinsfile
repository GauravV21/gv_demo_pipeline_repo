pipeline
{
agent any
stages
{
            stage('Welcome Stage')
            {
                        steps
                        {
                                    echo 'GV-Welcome to Jenkins Pipeline'
                        }
            }
           
            stage('Clean Stage')
            {
                        steps
                        {
                                    echo 'GV-This is Clean Stage'
                        }
                        }
stage('Clean Success Stage')
            {
                        steps
                        {
                                    echo 'GV-clean success'
                        }
            }          
            stage('Test Stage')
            {
                        steps
                        {
                                    echo 'GV-This is Clean Success Stage'
                        }
            }                      
            stage('Build & Install Stage')
            {
                        steps
                        {
                                    echo 'GV-This is Build Stage'
                        }
            }                      
           
            stage('Build Success')
            {
                        steps
                        {
                                    echo 'GV-Build successful'
                        }
            }                      
      }
}
